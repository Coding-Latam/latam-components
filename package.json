{
  "name": "latamcomponents",
  "version": "0.0.32",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/latamcomponents/latamcomponents.esm.js",
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./latam-button": {
      "import": "./dist/components/latam-button.js",
      "types": "./dist/components/latam-button.d.ts"
    },
    "./latam-faq": {
      "import": "./dist/components/latam-faq.js",
      "types": "./dist/components/latam-faq.d.ts"
    },
    "./latam-service": {
      "import": "./dist/components/latam-service.js",
      "types": "./dist/components/latam-service.d.ts"
    },
    "./latam-testimonial": {
      "import": "./dist/components/latam-testimonial.js",
      "types": "./dist/components/latam-testimonial.d.ts"
    },
    "./latam-project": {
      "import": "./dist/components/latam-project.js",
      "types": "./dist/components/latam-project.d.ts"
    },
    "./latam-strategy-card": {
      "import": "./dist/components/latam-strategy-card.js",
      "types": "./dist/components/latam-strategy-card.d.ts"
    },
    "./latam-team": {
      "import": "./dist/components/latam-team.js",
      "types": "./dist/components/latam-team.d.ts"
    }
  },
  "types": "dist/components/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coding-Latam/latam-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "stencil-tailwind-plugin": "^1.8.0",
    "tailwindcss": "^3.4.4",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "nuke": "rimraf node_modules rimraf package-lock.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-interactions": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/blocks": "^8.2.2",
    "@storybook/html": "^8.2.2",
    "@storybook/html-webpack5": "^8.2.2",
    "@storybook/test": "^8.2.2",
    "@storybook/web-components": "^8.2.2",
    "@storybook/web-components-vite": "^8.2.2",
    "lit": "^3.1.4",
    "storybook": "^8.2.2",
    "storybook-addon-stencil": "^0.2.1"
  },
  "dependencies": {
    "stencil-tailwind-plugin": "^1.8.0"
  }
}
